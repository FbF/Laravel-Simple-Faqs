{
    "name": "fbf/laravel-simple-faqs",
    "description": "A Laravel 4 package to add simple database driven FAQs to a site",
    "authors": [
        {
            "name": "Neil Crookes",
            "email": "neil.crookes@fivebyfiveuk.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "4.0.x",
        "cviebrock/eloquent-sluggable": "1.0.*"
    },
    "autoload": {
        "classmap": [
            "src/migrations",
            "src/models"
        ],
        "psr-0": {
            "Fbf\\LaravelSimpleFaqs": "src/"
        }
    },
    "minimum-stability": "dev"
}